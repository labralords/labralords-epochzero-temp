{
  "name": "@labralords/client",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "clean": "rimraf dist tsconfig.tsbuildinfo",
    "dev": "vite",
    "compile": "vite build",
    "preview": "vite preview",
    "build": "pnpm clean && pnpm compile && vite build",
    "build:dev": "pnpm build --mode development",
    "build:prod": "pnpm build --mode production",
    "build:stage": "pnpm build --mode stage",
    "tt": "pnpm --silent --workspace-root pnpm-context -t services/client manifests/staticWeb.Dockerfile --list-files",
    "docker:build": "pnpm --silent --workspace-root pnpm-context -t services/client manifests/staticWeb.Dockerfile | docker build --platform linux/amd64 --build-arg PACKAGE_PATH=services/client - -t labralords-client",
    "docker:start": "docker run --rm -i --name=labralords-client -p 3000:3000 -e NODE_ENV=development labralords-client",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "format": "prettier '**/*.{js,ts,cjs}' --write --ignore-path ../../.gitignore",
    "format-check": "prettier --list-different '**/*.{js,ts,cjs}' --ignore-path ../../.gitignore",
    "lint": "eslint . --ext js,ts,cjs --config .eslintrc.cjs --ignore-path ../../.gitignore",
    "lint-fix": "pnpm lint -- --fix",
    "depcheck": "depcheck ."
  },
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "^1.0.1",
    "@tsconfig/svelte": "^3.0.0",
    "@types/lodash": "^4.14.182",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.27.1",
    "@typescript-eslint/parser": "^5.27.1",
    "autoprefixer": "^10.4.7",
    "browserify-zlib": "^0.2.0",
    "depcheck": "^1.4.3",
    "eslint": "^8.17.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^2.7.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-security": "^1.5.0",
    "eslint-plugin-unicorn": "^42.0.0",
    "flowbite": "^1.4.7",
    "flowbite-svelte": "^0.24.9",
    "postcss": "^8.4.14",
    "prettier": "^2.6.2",
    "rimraf": "^3.0.2",
    "stream-browserify": "^3.0.0",
    "svelte": "^3.49.0",
    "svelte-check": "^2.8.0",
    "svelte-preprocess": "^4.10.7",
    "tailwindcss": "^3.1.6",
    "tslib": "^2.4.0",
    "typescript": "^4.6.4",
    "vite": "^3.0.0"
  },
  "dependencies": {
    "@apollo/client": "^3.6.9",
    "@fortawesome/free-brands-svg-icons": "^6.1.1",
    "@fortawesome/free-solid-svg-icons": "^6.1.1",
    "@labralords/common": "workspace:*",
    "buffer": "^6.0.3",
    "date-fns": "^2.29.3",
    "echarts": "^5.3.3",
    "events": "^3.3.0",
    "graphql": "^16.5.0",
    "lodash": "^4.17.21",
    "process": "^0.11.10",
    "svelte-apollo": "^0.5.0",
    "svelte-fa": "^3.0.3",
    "svelte-heros": "^2.3.3",
    "svelte-i18n": "^3.4.0",
    "svelte-navigator": "^3.1.6",
    "util": "^0.12.4",
    "uuid": "^8.3.2",
    "web3": "^1.7.5"
  }
}
